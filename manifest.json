{
  "name": "Search Naver English-Korean Dictionary",
  "description": "Quickly look up words on Naver's English-Korean dictionary (en.dict.naver.com)",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon-128.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}